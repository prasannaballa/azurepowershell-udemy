# Hash tables
# key/values -- Comments
$serverNames = @{
            Devlopment= 'server01'
            Testing = 'server02'
            Production = 'server03'
}
$serverNames['Devlopment']
$serverNames.Devlopment
$serverNames.Add('QA','server04')
$serverNames

